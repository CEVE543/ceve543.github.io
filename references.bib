@article{ramshani_optimcalplanningunderclimatechange:2020,
  title = {Optimal Planning of the Joint Placement of Photovoltaic Panels and Green Roofs under Climate Change Uncertainty},
  author = {Ramshani, Mohammad and Khojandi, Anahita and Li, Xueping and Omitaomu, Olufemi},
