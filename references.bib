@article{pierce_improved:2015,
  title = {Improved Bias Correction Techniques for Hydrological Simulations of Climate Change},
  author = {Pierce, David W. and Cayan, Daniel R. and Maurer, Edwin P. and Abatzoglou, John T. and Hegewisch, Katherine C.},
  year = {2015},
  month = sep,
  journal = {Journal of Hydrometeorology},
  volume = {16},
