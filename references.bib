@article{huang_transition:2005,
  title = {The 1976/77 Transition in Precipitation over the {{Americas}} and the Influence of Tropical Sea Surface Temperature},
  author = {Huang, Huei-Ping and Seager, Richard and Kushnir, Yochanan},
  year = {2005},
