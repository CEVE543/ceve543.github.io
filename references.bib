@article{xu_rwh:2020,
  title = {Real Time Control of Rainwater Harvesting Systems: The Benefits of Increasing Rainfall Forecast Window},
  author = {Xu, Wei D. and Fletcher, Tim D. and Burns, Matthew J. and Cherqui, Fr{\'e}d{\'e}ric},
