@article{Tedeschi:2013ej,
  title = {Influences of Two Types of {{ENSO}} on {{South American}} Precipitation},
  author = {Tedeschi, Renata G and Cavalcanti, Iracema F A and Grimm, Alice M},
  year = {2013},
