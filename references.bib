@article{ennenbach_rainwater:2018,
  title = {County-{{Scale Rainwater Harvesting Feasibility}} in the {{United States}}: {{Climate}}, {{Collection Area}}, {{Density}}, and {{Reuse Considerations}}},
  author = {Ennenbach, Mounir William and Larrauri, Paulina Concha and Lall, Upmanu},
  year = {2018},
