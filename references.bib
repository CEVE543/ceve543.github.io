@article{najibi:2019,
  title = {Coupled Flow Accumulation and Atmospheric Blocking Govern Flood Duration},
  author = {Najibi, Nasser and Devineni, Naresh and Lu, Mengqian and Perdig{\~a}o, Rui A. P.},
  year = {2019},
  month = jun,
  journal = {npj Climate and Atmospheric Science},
  volume = {2},
  number = {1},
  pages = {19},
  issn = {2397-3722},
  doi = {10.1038/s41612-019-0076-6},
  urldate = {2019-06-21},
  abstract = {The duration of floods are controlled by river flow, precipitation and atmospheric blocking. An international team of researchers led by Nasser Najibi at the City University of New York have developed a regional scale predictive model to examine the timescales of flooding. The statistical model is tested on the Missouri River Basin. Long duration floods first require high flow conditions in rivers created by recurrent high intensity rainfall events, which is then followed by a large stable long-lived low-pressure system \textemdash{} a storm cell. These conditions may then result in large-scale devastating floods. In shorter duration floods, however, this land\textendash atmospheric coupling is negligible thus explaining why not all storms result in widespread flooding. Research into predicting flooding duration may help us to mitigate and plan for future flooding events as the climate changes.},
  copyright = {2019 The Author(s)}
}
