@article{jacobson_100:2017,
  title = {100\% {{Clean}} and {{Renewable Wind}}, {{Water}}, and {{Sunlight All-Sector Energy Roadmaps}} for 139 {{Countries}} of the {{World}}},
  author = {Jacobson, Mark Z. and Delucchi, Mark A. and Bauer, Zack A. F. and Goodman, Savannah C. and Chapman, William E. and Cameron, Mary A. and Bozonnat, Cedric and Chobadi, Liat and Clonts, Hailey A. and Enevoldsen, Peter and Erwin, Jenny R. and Fobi, Simone N. and Goldstrom, Owen K. and Hennessy, Eleanor M. and Liu, Jingyi and Lo, Jonathan and Meyer, Clayton B. and Morris, Sean B. and Moy, Kevin R. and O'Neill, Patrick L. and Petkov, Ivalin and Redfern, Stephanie and Schucker, Robin and Sontag, Michael A. and Wang, Jingfan and Weiner, Eric and Yachanin, Alexander S.},
  year = {2017},
  month = sep,
  journal = {Joule},
  volume = {1},
  number = {1},
  pages = {108--121},
  issn = {2542-4351},
  doi = {10.1016/j.joule.2017.07.005},
  urldate = {2020-01-13},
  abstract = {We develop roadmaps to transform the all-purpose energy infrastructures (electricity, transportation, heating/cooling, industry, agriculture/forestry/fishing) of 139 countries to ones powered by wind, water, and sunlight (WWS). The roadmaps envision 80\% conversion by 2030 and 100\% by 2050. WWS not only replaces business-as-usual (BAU) power, but also reduces it {$\sim$}42.5\% because the work: energy ratio of WWS electricity exceeds that of combustion (23.0\%), WWS requires no mining, transporting, or processing of fuels (12.6\%), and WWS end-use efficiency is assumed to exceed that of BAU (6.9\%). Converting may create {$\sim$}24.3 million more permanent, full-time jobs than jobs lost. It~may avoid {$\sim$}4.6 million/year premature air-pollution deaths today and {$\sim$}3.5~million/year in 2050; {$\sim\$$}22.8 trillion/year (12.7 \textcent/kWh-BAU-all-energy) in 2050 air-pollution costs; and {$\sim\$$}28.5 trillion/year (15.8 \textcent/kWh-BAU-all-energy) in 2050 climate costs. Transitioning should also stabilize energy prices because fuel costs are zero, reduce power disruption and increase access to energy by decentralizing power, and avoid 1.5\textdegree C global warming.}
}
