@article{Ward:2016fc,
  title = {Flood {{Frequencies}} and {{Durations}} and {{Their Response}} to {{El Ni\~no Southern Oscillation}}: {{Global Analysis}}},
  author = {Ward, P J and Kummu, M and Lall, Upmanu},
  year = {2016},
  journal = {Journal of Hydrology},
