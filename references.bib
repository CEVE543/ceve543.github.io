@incollection{reiss_objectivity:2020,
  title = {Scientific {{Objectivity}}},
  booktitle = {The {{Stanford Encyclopedia}} of {{Philosophy}}},
  author = {Reiss, Julian and Sprenger, Jan},
  editor = {Zalta, Edward N.},
  year = {2020},
  edition = {Winter 2020},
  publisher = {{Metaphysics Research Lab, Stanford University}},
  urldate = {2020-11-04},
  abstract = {Scientific objectivity is a property of various aspects of science. Itexpresses the idea that scientific claims, methods, results\textemdash andscientists themselves\textemdash are not, or should not be, influenced byparticular perspectives, value judgments, community bias or personalinterests, to name a few relevant factors. Objectivity is oftenconsidered to be an ideal for scientific inquiry, a good reason forvaluing scientific knowledge, and the basis of the authority ofscience in society., Many central debates in the philosophy of science have, in one way oranother, to do with objectivity: confirmation and the problem ofinduction; theory choice and scientific change; realism; scientificexplanation; experimentation; measurement and quantification;statistical evidence; reproducibility; evidence-based science;feminism and values in science. Understanding the role of objectivityin science is therefore integral to a full appreciation of thesedebates. As this article testifies, the reverse is true too: it isimpossible to fully appreciate the notion of scientific objectivitywithout touching upon many of these debates., The ideal of objectivity has been criticized repeatedly in philosophyof science, questioning both its desirability and its attainability.This article focuses on the question of how scientific objectivityshould be defined, whether the ideal of objectivity isdesirable, and to what extent scientists can achieveit.}
}
