@article{meinshausen_pledges:2022,
  title = {Realization of {{Paris Agreement}} Pledges May Limit Warming Just below 2 \textdegree{{C}}},
  author = {Meinshausen, Malte and Lewis, Jared and McGlade, Christophe and G{\"u}tschow, Johannes and Nicholls, Zebedee and Burdon, Rebecca and Cozzi, Laura and Hackmann, Bernd},
  year = {2022},
