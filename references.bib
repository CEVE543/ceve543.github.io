@article{vergopolan_smaphydroblocks:2021,
  title = {{{SMAP-HydroBlocks}}, a 30-m Satellite-Based Soil Moisture Dataset for the Conterminous {{US}}},
  author = {Vergopolan, Noemi and Chaney, Nathaniel W. and Pan, Ming and Sheffield, Justin and Beck, Hylke E. and Ferguson, Craig R. and {Torres-Rojas}, Laura and Sadri, Sara and Wood, Eric F.},
  year = {2021},
  month = oct,
  journal = {Scientific Data},
  volume = {8},
  number = {1},
  pages = {264},
  publisher = {{Nature Publishing Group}},
  issn = {2052-4463},
  doi = {10.1038/s41597-021-01050-2},
  urldate = {2023-03-01},
  abstract = {Soil moisture plays a key role in controlling land-atmosphere interactions, with implications for water resources, agriculture, climate, and ecosystem dynamics. Although soil moisture varies strongly across the landscape, current monitoring capabilities are limited to coarse-scale satellite retrievals and a few regional in-situ networks. Here, we introduce SMAP-HydroBlocks (SMAP-HB), a high-resolution satellite-based surface soil moisture dataset at an unprecedented 30-m resolution (2015\textendash 2019) across the conterminous United States. SMAP-HB was produced by using a scalable cluster-based merging scheme that combines high-resolution land surface modeling, radiative transfer modeling, machine learning, SMAP satellite microwave data, and in-situ observations. We evaluated the resulting dataset over 1,192 observational sites. SMAP-HB performed substantially better than the current state-of-the-art SMAP products, showing a median temporal correlation of 0.73\,{$\pm$}\,0.13 and a median Kling-Gupta Efficiency of 0.52\,{$\pm$}\,0.20. The largest benefit of SMAP-HB is, however, the high spatial detail and improved representation of the soil moisture spatial variability and spatial accuracy with respect to SMAP products. The SMAP-HB dataset is available via zenodoand at https://waterai.earth/smaphb.},
  copyright = {2021 The Author(s)}
}
