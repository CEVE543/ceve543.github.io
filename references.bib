@article{cioffi_tanzania:2016,
  title = {Projecting Changes in {{Tanzania}} Rainfall for the 21st Century},
  author = {Cioffi, F. and Conticello, F. and Lall, U.},
  year = {2016},
  journal = {International Journal of Climatology},
