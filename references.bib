@article{hofmann_boxenplot:2017,
  title = {Letter-{{Value Plots}}: {{Boxplots}} for {{Large Data}}},
  author = {Hofmann, Heike and Wickham, Hadley and Kafadar, Karen},
  year = {2017},
  month = jul,
  journal = {Journal of Computational and Graphical Statistics},
  volume = {26},
