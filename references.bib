@article{bates_modeling:2021,
  title = {Combined Modeling of {{US}} Fluvial, Pluvial, and Coastal Flood Hazard under Current and Future Climates},
  author = {Bates, Paul D. and Quinn, Niall and Sampson, Christopher and Smith, Andrew and Wing, Oliver and Sosa, Jeison and Savage, James and Olcese, Gaia and Neal, Jeff and Schumann, Guy and Giustarini, Laura and Coxon, Gemma and Porter, Jeremy R. and Amodeo, Mike F. and Chu, Ziyan and {Lewis-Gruss}, Sharai and Freeman, Neil B. and Houser, Trevor and Delgado, Michael and Hamidi, Ali and Bolliger, Ian and E. McCusker, Kelly and Emanuel, Kerry and Ferreira, Celso M. and Khalid, Arslaan and Haigh, Ivan D. and Couasnon, Ana{\"i}s and E. Kopp, Robert and Hsiang, Solomon and Krajewski, Witold F.},
  year = {2021},
  journal = {Water Resources Research},
