@article{sevcikova_melding:2007,
  title = {Assessing Uncertainty in Urban Simulations Using {{Bayesian}} Melding},
  author = {{\v S}ev{\v c}{\'i}kov{\'a}, Hana and Raftery, Adrian E. and Waddell, Paul A.},
  year = {2007},
  month = jul,
  journal = {Transportation Research Part B: Methodological},
  volume = {41},
