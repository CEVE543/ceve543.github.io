@article{herdies_budget:2002,
  title = {Moisture Budget of the Bimodal Pattern of the Summer Circulation over {{South America}}},
  author = {Herdies, Dirceu L. and {da Silva}, Arlindo and Silva Dias, Maria A. F. and Nieto Ferreira, Rosana},
  year = {2002},
