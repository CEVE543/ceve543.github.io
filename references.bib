@article{Aerts:2018ci,
  title = {Integrating Human Behaviour Dynamics into Flood Disaster Risk Assessment},
  author = {Aerts, Jeroen C J H and Botzen, W J Wouter and Clarke, K C and Cutter, S L and Hall, J W and Merz, B and {Michel-Kerjan}, E and Mysiak, J and Surminski, S and Kunreuther, H},
