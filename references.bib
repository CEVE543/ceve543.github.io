@article{kabir_belief:2015,
  title = {Evaluating Risk of Water Mains Failure Using a {{Bayesian}} Belief Network Model},
  author = {Kabir, Golam and Tesfamariam, Solomon and Francisque, Alex and Sadiq, Rehan},
