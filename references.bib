@article{williams_decarbonization:2012,
  title = {The Technology Path to Deep Greenhouse Gas Emissions Cuts by 2050: The Pivotal Role of Electricity},
  author = {Williams, James H. and DeBenedictis, Andrew and Ghanadan, Rebecca and Mahone, Amber and Moore, Jack and Morrow, William R. and Price, Snuller and Torn, Margaret S.},
  year = {2012},
