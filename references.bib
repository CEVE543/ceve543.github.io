@article{evensen_enks:2000,
  title = {An {{Ensemble Kalman Smoother}} for {{Nonlinear Dynamics}}},
  author = {Evensen, Geir and van Leeuwen, Peter Jan},
  year = {2000},
  month = jun,
  journal = {Monthly Weather Review},
  volume = {128},
  number = {6},
  pages = {1852--1867},
