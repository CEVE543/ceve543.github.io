@article{hadka_multimaster:2015,
  title = {Large-Scale Parallelization of the {{Borg}} Multiobjective Evolutionary Algorithm to Enhance the Management of Complex Environmental Systems},
  author = {Hadka, David and Reed, Patrick},
  year = {2015},
  month = jul,
  journal = {Environmental Modelling \& Software},
  volume = {69},
  pages = {353--369},
  issn = {1364-8152},
  doi = {10.1016/j.envsoft.2014.10.014},
  urldate = {2020-07-14},
  abstract = {The Borg MOEA is a self-adaptive multiobjective evolutionary algorithm capable of solving complex, many-objective environmental systems problems efficiently and reliably. Water and environmental resources problems pose significant computational challenges due to their potential for large Pareto optimal sets, the presence of disjoint Pareto-optimal regions that arise from discrete choices, multi-modal suboptimal regions, and expensive objective function calculations. This work develops two large-scale parallel implementations of the Borg MOEA, the master\textendash slave and multi-master Borg MOEA, and applies them to a highly challenging risk-based water supply portfolio planning problem. The performance and scalability of both implementations are compared on up to 16384 processors. The multi-master Borg MOEA is shown to scale efficiently on tens of thousands of cores while dramatically improving the reliability of attaining high-quality solutions. Our results dramatically expand the scale and scope of complex environmental systems that can be addressed using many-objective evolutionary optimization.}
}
