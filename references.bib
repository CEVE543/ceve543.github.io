@article{carter_grrien:2023,
  title = {{{GRRIEn}} Analysis: A Data Science Cheat Sheet for Earth Scientists Learning from Global Earth Observations},
  author = {Carter, Elizabeth and Hultquist, Carolynne and Wen, Tao},
  year = {2023},
