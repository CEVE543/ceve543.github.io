@article{armal_impacts:2020,
  title = {Assessing {{Property Level Economic Impacts}} of {{Climate}} in the {{US}}, {{New Insights}} and {{Evidence}} from a {{Comprehensive Flood Risk Assessment Tool}}},
  author = {Armal, Saman and Porter, Jeremy R. and Lingle, Brett and Chu, Ziyan and Marston, Michael L. and Wing, Oliver E. J.},
  year = {2020},
  month = oct,
  journal = {Climate},
  volume = {8},
