@article{chafik_northsea:2017,
  title = {Impact of {{North Atlantic}} Teleconnection Patterns on Northern {{European}} Sea Level},
  author = {Chafik, L{\'e}on and Nilsen, Jan Even {\O}ie and Dangendorf, S{\"o}nke},
  year = {2017},
