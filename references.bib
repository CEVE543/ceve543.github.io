@article{brody_houston:2018,
  title = {Case Study Results from Southeast {{Houston}}, {{Texas}}: Identifying the Impacts of Residential Location on Flood Risk and Loss},
  author = {Brody, S. D. and Sebastian, A. and Blessing, R. and Bedient, P. B.},
  year = {2018},
