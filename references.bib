@article{dorenkamper_windatlas:2020,
  title = {The {{Making}} of the {{New European Wind Atlas}} \textendash{} {{Part}} 2: {{Production}} and Evaluation},
  author = {D{\"o}renk{\"a}mper, Martin and Olsen, Bjarke T. and Witha, Bj{\"o}rn and Hahmann, Andrea N. and Davis, Neil N. and Barcons, Jordi and Ezber, Yasemin and {Garc{\'i}a-Bustamante}, Elena and {Gonz{\'a}lez-Rouco}, J. Fidel and Navarro, Jorge and {Sastre-Marug{\'a}n}, Mariano and S{\=i}le, Tija and Trei, Wilke and {\v Z}agar, Mark and Badger, Jake and Gottschall, Julia and Sanz Rodrigo, Javier and Mann, Jakob},
  year = {2020},
  month = oct,
  journal = {Geoscientific Model Development},
  volume = {13},
  number = {10},
  pages = {5079--5102},
  issn = {1991-9603},
  doi = {10.5194/gmd-13-5079-2020},
  urldate = {2023-06-14},
  abstract = {Abstract. This is the second of two papers that document the creation of the New European Wind Atlas (NEWA). In Part~1, we described the sensitivity experiments and accompanying evaluation done to arrive at the final mesoscale model setup used to produce the mesoscale wind atlas. In this paper, Part~2, we document how we made the final wind atlas product, covering both the production of the mesoscale climatology generated with the Weather Research and Forecasting (WRF) model and the microscale climatology generated with the Wind Atlas Analysis and Applications Program (WAsP). The paper includes a detailed description of the technical and practical aspects that went into running the mesoscale simulations and the downscaling using WAsP. We show the main results from the final wind atlas and present a comprehensive evaluation of each component of the NEWA model chain using observations from a large set of tall masts located all over Europe. The added value of the WRF and WAsP downscaling of wind climatologies is evaluated relative to the performance of the driving ERA5 reanalysis and shows that the WRF downscaling reduces the mean wind speed bias and spread relative to that of ERA5 from -1.50{$\pm$}1.30 to 0.02{$\pm$}0.78\,m\,s-1. The WAsP downscaling has an added positive impact relative to that of the WRF model in simple terrain. In complex terrain, where the assumptions of the linearized flow model break down, both the mean bias and spread in wind speed are worse than those from the raw mesoscale results.}
}
