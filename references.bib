@article{karpatne_geosciml:2017,
  title = {Machine {{Learning}} for the {{Geosciences}}: {{Challenges}} and {{Opportunities}}},
  author = {Karpatne, Anuj and {Ebert-Uphoff}, Imme and Ravela, Sai and Babaie, Hassan Ali and Kumar, Vipin},
