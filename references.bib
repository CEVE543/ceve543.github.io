@article{bertolacci_adaptspecx:2019,
  title = {{{AdaptSPEC-X}}: {{Covariate Dependent Spectral Modeling}} of {{Multiple Nonstationary Time Series}}},
  author = {Bertolacci, Michael and Rosen, Ori and Cripps, Edward and Cripps, Sally},
  year = {2019},
