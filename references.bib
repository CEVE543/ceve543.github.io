@article{rueda_surge:2016,
  title = {A Multivariate Extreme Wave and Storm Surge Climate Emulator Based on Weather Patterns},
  author = {Rueda, A. and Camus, P. and Tom{\'a}s, A. and Vitousek, S. and M{\'e}ndez, F. J.},
