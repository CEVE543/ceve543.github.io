@article{teodoro_waterandseweraffordabilityintheunitedstates:2020,
  title = {Water and Sewer Affordability in the {{United States}}: A 2019 Update},
  author = {Teodoro, Manuel P. and Saywitz, Robin Rose},
  year = {2020},
  journal = {AWWA Water Science},
  volume = {2},
  number = {2},
  pages = {e1176},
  issn = {2577-8161},
  doi = {10.1002/aws2.1176},
  urldate = {2022-09-11},
  abstract = {This study uses contemporary metrics and data from an original, representative sample of water and sewer utilities in the United States to calculate the affordability of basic single-family residential water and sewer services for low-income households in 2019. Results show that affordability conditions have worsened on average since the last such study in 2017: low-income households must spend an average of 12.4\% of their disposable income (up from 10.9\%) and/or work 10.1 h at minimum wage (up from 9.9 h) to pay for basic monthly water and sewer services. Analysis suggests that rising prices combine with underlying economic trends to exacerbate affordability.}
}
