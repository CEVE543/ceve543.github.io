@article{loew_thermal:2020,
  title = {Fossil Fuel\textendash Fired Power Plant Operations under a Changing Climate},
  author = {Loew, Aviva and Jaramillo, Paulina and Zhai, Haibo and Ali, Rahim and Nijssen, Bart and Cheng, Yifan and Klima, Kelly},
  year = {2020},
