@article{koutsoyiannis_idf:1998,
  title = {A Mathematical Framework for Studying Rainfall Intensity-Duration-Frequency Relationships},
  author = {Koutsoyiannis, Demetris and Kozonis, Demosthenes and Manetas, Alexandros},
  year = {1998},
