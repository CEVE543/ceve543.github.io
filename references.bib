@article{bengio_flownetwork:2021,
  title = {Flow Network Based Generative Nodels for Non-Iterative Diverse Candidate Generation},
  author = {Bengio, Emmanuel and Jain, Moksh and Korablyov, Maksym and Precup, Doina and Bengio, Yoshua},
  year = {2021},
