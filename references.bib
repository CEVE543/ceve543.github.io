@article{calafat_surgetrends:2022,
  title = {Trends in {{Europe}} Storm Surge Extremes Match the Rate of Sea-Level Rise},
  author = {Calafat, Francisco M. and Wahl, Thomas and Tadesse, Michael Getachew and Sparrow, Sarah N.},
  year = {2022},
