@article{hino_managedretreat:2017,
  title = {Managed Retreat as a Response to Natural Hazard Risk},
  author = {Hino, Miyuki and Field, Christopher B. and Mach, Katharine J.},
  year = {2017},
  month = may,
  journal = {Nature Climate Change},
  volume = {7},
  number = {5},
  pages = {364--370},
  issn = {1758-6798},
  doi = {10.1038/nclimate3252},
  urldate = {2019-03-21},
  abstract = {Managed retreat is a potentially important climate change adaptation option, providing an alternative to structural protection or accommodation measures to manage natural hazard risk. However, its application faces challenges given the projected scale of climate-induced displacement and the difficulties of resettlement. We evaluate the drivers, barriers and outcomes of 27 recent cases of managed retreat that have resettled approximately 1.3 million people. A conceptual model based on two key factors\textemdash who benefits from retreat and who initiates it\textemdash organizes the diverse set of cases into four quadrants. Different sociopolitical dimensions emerge as particularly influential in each quadrant. The model establishes a foundation for understanding and anticipating case-specific complexities. It can be used to unpack the landscape of managed retreat and evaluate its potential future applications.},
  copyright = {2017 Nature Publishing Group}
}
