  author = {Zhang, Jian and Howard, Kenneth and Langston, Carrie and Vasiloff, Steve and Kaney, Brian and Arthur, Ami and Cooten, Suzanne Van and Kelleher, Kevin and Kitzmiller, David and Ding, Feng and Seo, Dong-Jun and Wells, Ernie and Dempsey, Chuck},
