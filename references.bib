@article{cagigal_emulator:2020,
  title = {A Multivariate, Stochastic, Climate-Based Wave Emulator for Shoreline Change Modelling},
  author = {Cagigal, Laura and Rueda, Ana and Anderson, Dylan and Ruggiero, Peter and Merrifield, Mark A. and Monta{\~n}o, Jennifer and Coco, Giovanni and M{\'e}ndez, Fernando J.},
  year = {2020},
