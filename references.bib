@article{tobin_vulnerabilities:2018,
  title = {Vulnerabilities and Resilience of {{European}} Power Generation to 1.5 \textdegree{{C}}, 2 \textdegree{{C}} and 3 \textdegree{{C}} Warming},
  author = {Tobin, I. and Greuell, W. and Jerez, S. and Ludwig, F. and Vautard, R. and van Vliet, M. T. H. and Br{\'e}on, F.-M.},
  year = {2018},
  month = apr,
  journal = {Environmental Research Letters},
  volume = {13},
  number = {4},
  pages = {044024},
  publisher = {{IOP Publishing}},
  issn = {1748-9326},
  doi = {10.1088/1748-9326/aab211},
  urldate = {2023-04-06},
  abstract = {The electricity sector is currently considered mainly on the emission side of the climate change equation. In order to limit climate warming to below 2 \textdegree C, or even 1.5 \textdegree C, it must undergo a rapid transition towards carbon neutral production by the mid-century. Simultaneously, electricity generating technologies will be vulnerable to climate change. Here, we assess the impacts of climate change on wind, solar photovoltaic, hydro and thermoelectric power generation in Europe using a consistent modelling approach across the different technologies. We compare the impacts for different global warming scenarios: +1.5 \textdegree C, +2 \textdegree C and +3 \textdegree C. Results show that climate change has negative impacts on electricity production in most countries and for most technologies. Such impacts remain limited for a 1.5 \textdegree C warming, and roughly double for a 3 \textdegree C warming. Impacts are relatively limited for solar photovoltaic and wind power potential which may reduce up to 10\%, while hydropower and thermoelectric generation may decrease by up to 20\%. Generally, impacts are more severe in southern Europe than in northern Europe, inducing inequity between EU countries. We show that a higher share of renewables could reduce the vulnerability of power generation to climate change, although the variability of wind and solar PV production remains a significant challenge.}
}
