@article{dye-braumuller_vectorcontrol:2022,
  title = {Needs Assessment of {{Southeastern United States}} Vector Control Agencies: Capacity Improvement Is Greatly Needed to Prevent the next Vector-Borne Disease Outbreak},
  author = {{Dye-Braumuller}, Kyndall C. and Gordon, Jennifer R. and Johnson, Danielle and Morrissey, Josie and McCoy, Kaci and Dinglasan, Rhoel R. and Nolan, Melissa S.},
