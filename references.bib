  title = {Resolving {{Long-Term Variations}} in {{North Atlantic Tropical Cyclone Activity Using}} a {{Pseudo Proxy Paleotempestology Network Approach}}},
  author = {Wallace, Elizabeth J. and Dee, Sylvia G. and Emanuel, Kerry A.},
  year = {2021},
