@article{ahmad_nuclear:2021,
  title = {Increase in Frequency of Nuclear Power Outages Due to Changing Climate},
  author = {Ahmad, Ali},
  year = {2021},
  month = jul,
  journal = {Nature Energy},
  volume = {6},
  number = {7},
  pages = {755--762},
