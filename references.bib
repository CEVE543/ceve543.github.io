@article{bird_lafourche:2019,
  title = {Water Pollution and Observation of Acquired Antibiotic Resistance in {{Bayou Lafourche}}, a Major Drinking Water Source in {{Southeast Louisiana}}, {{USA}}},
  author = {Bird, Kyle and Boopathy, Raj and Nathaniel, Rajkumar and LaFleur, Gary},
  year = {2019},
  month = nov,
  journal = {Environmental Science and Pollution Research},
  volume = {26},
  number = {33},
  pages = {34220--34232},
