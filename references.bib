@article{bregy_tcprecip:2020,
  title = {Spatiotemporal Variability of Tropical Cyclone Precipitation Using a High-Resolution, Gridded (0.25\textdegree{} \texttimes{} 0.25\textdegree ) Dataset for the Eastern {{United States}}, 1948\textendash 2015},
  author = {Bregy, Joshua C. and Maxwell, Justin T. and Robeson, Scott M. and Ortegren, Jason T. and Soul{\'e}, Peter T. and Knapp, Paul A.},
  year = {2020},
  month = jan,
  journal = {Journal of Climate},
  volume = {33},
  number = {5},
  pages = {1803--1819},
  publisher = {{American Meteorological Society}},
  issn = {0894-8755, 1520-0442},
  doi = {10.1175/JCLI-D-18-0885.1},
  urldate = {2022-11-13},
  abstract = {Abstract Tropical cyclones (TCs) are an important source of precipitation for much of the eastern United States. However, our understanding of the spatiotemporal variability of tropical cyclone precipitation (TCP) and the connections to large-scale atmospheric circulation is limited by irregularly distributed rain gauges and short records of satellite measurements. To address this, we developed a new gridded (0.25\textdegree{} \texttimes{} 0.25\textdegree ) publicly available dataset of TCP (1948\textendash 2015; Tropical Cyclone Precipitation Dataset, or TCPDat) using TC tracks to identify TCP within an existing gridded precipitation dataset. TCPDat was used to characterize total June\textendash November TCP and percentage contribution to total June\textendash November precipitation. TCP totals and contributions had maxima on the Louisiana, North Carolina, and Texas coasts, substantially decreasing farther inland at rates of approximately 6.2\textendash 6.7 mm km-1. Few statistically significant trends were discovered in either TCP totals or percentage contribution. TCP is positively related to an index of the position and strength of the western flank of the North Atlantic subtropical high (NASH), with the strongest correlations concentrated in the southeastern United States. Weaker inverse correlations between TCP and El Ni\~no\textendash Southern Oscillation are seen throughout the study site. Ultimately, spatial variations of TCP are more closely linked to variations in the NASH flank position or strength than to the ENSO index. The TCP dataset developed in this study is an important step in understanding hurricane\textendash climate interactions and the impacts of TCs on communities, water resources, and ecosystems in the eastern United States.},
  chapter = {Journal of Climate}
}
