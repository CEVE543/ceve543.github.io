@article{bessette_vimm:2017,
  title = {Building a Values-Informed Mental Model for {{New Orleans}} Climate Risk Management},
  author = {Bessette, Douglas L. and Mayer, Lauren A. and Cwik, Bryan and Vez{\'e}r, Martin and Keller, Klaus and Lempert, Robert J. and Tuana, Nancy},
