@misc{chapman_adaptive:2023,
  title = {Bridging Adaptive Management and Reinforcement Learning for More Robust Decisions},
  author = {Chapman, Melissa and Xu, Lily and Lapeyrolerie, Marcus and Boettiger, Carl},
  year = {2023},
