  author = {Stone, Brian Jr. and Gronlund, Carina J. and Mallen, Evan and Hondula, David and O'Neill, Marie S. and Rajput, Mayuri and Grijalva, Santiago and Lanza, Kevin and Harlan, Sharon and Larsen, Larissa and Augenbroe, Godfried and Krayenhoff, E. Scott and Broadbent, Ashley and Georgescu, Matei},
