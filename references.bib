@article{serdeczny_africa:2017,
  title = {Climate Change Impacts in {{Sub-Saharan Africa}}: From Physical Changes to Their Social Repercussions},
  author = {Serdeczny, Olivia and Adams, Sophie and Baarsch, Florent and Coumou, Dim and Robinson, Alexander and Hare, William and Schaeffer, Michiel and Perrette, Mah{\'e} and Reinhardt, Julia},
